{
  "name": "hyper.chrome",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Hyper client for chrome",
  "app": {
    "launch": {
      "local_path": "index.html"
    }
  },
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "content_security_policy": "default-src 'self'; connect-src *; script-src 'self' 'unsafe-eval'; img-src *;",
  "offline_enabled": true
}
